@model string
<td class="text-right">
    <div class="btn-group" role="group">
       
        <form method="post" action="Delete">
            <button onclick="if (!confirm('Are you sure you want to delete this')) return false" type="submit" class="btn btn-danger m-1" asp-action="Delete" asp-route-id="@Model"><i class="far fa-trash-alt"></i>&nbsp; Delete</button>
        </form>
    </div>
</td>